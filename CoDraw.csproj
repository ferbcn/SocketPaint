<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.3"/>
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0"/>
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="react-old\package.json" />
      <_ContentIncludedByDefault Remove="react-old\public\manifest.json" />
    </ItemGroup>

    <ItemGroup>
      <Content Update="wwwroot\build\manifest.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
      <Content Update="wwwroot\build\asset-manifest.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
      <Content Update="wwwroot\build\asset-manifest.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
      <Content Update="wwwroot\build\manifest.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

    <ItemGroup>
      <None Include="wwwroot\build\favicon.ico" />
      <None Include="wwwroot\build\index.html" />
      <None Include="wwwroot\build\logo192.png" />
      <None Include="wwwroot\build\logo512.png" />
      <None Include="wwwroot\build\robots.txt" />
      <None Include="wwwroot\build\static\css\main.3a6b33a4.css" />
      <None Include="wwwroot\build\static\css\main.3a6b33a4.css.map" />
      <None Include="wwwroot\build\static\js\453.f9d3e484.chunk.js" />
      <None Include="wwwroot\build\static\js\453.f9d3e484.chunk.js.map" />
      <None Include="wwwroot\build\static\js\main.f0f8db5f.js" />
      <None Include="wwwroot\build\static\js\main.f0f8db5f.js.LICENSE.txt" />
      <None Include="wwwroot\build\static\js\main.f0f8db5f.js.map" />
      <None Include="wwwroot\build\static\media\logo.6ce24c58023cc2f8fd88fe9d219db6c6.svg" />
    </ItemGroup>
</Project>
